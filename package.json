{
  "name": "obind",
  "version": "0.2.0",
  "description": "Partial application for functions taking a single argument object",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R spec test/*spec.js",
    "examples": "xplain -i test/spec.js -o examples.md",
    "commit": "commit-wizard"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bahmutov/obind.git"
  },
  "keywords": [
    "partial",
    "application",
    "function",
    "argument",
    "options",
    "object",
    "single"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/obind/issues"
  },
  "homepage": "https://github.com/bahmutov/obind",
  "devDependencies": {
    "check-more-types": "2.1.2",
    "lazy-ass": "0.6.0",
    "mocha": "2.3.4",
    "pre-git": "1.2.1",
    "xplain": "0.5.4"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test",
        "npm run examples",
        "npm version"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "czConfig": {
    "path": "node_modules/pre-git/node_modules/cz-conventional-changelog"
  }
}
