{
  "name": "obind",
  "description": "Partial application for functions taking a single argument object",
  "main": "index.js",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "test": "mocha test/*spec.js",
    "examples": "xplain -i test/spec.js -o examples.md",
    "commit": "git-issues && commit-wizard",
    "issues": "git-issues",
    "prepublish": "pkgfiles",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/obind.git"
  },
  "keywords": [
    "partial",
    "application",
    "function",
    "argument",
    "options",
    "object",
    "single"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/obind/issues"
  },
  "homepage": "https://github.com/bahmutov/obind",
  "devDependencies": {
    "check-more-types": "2.3.0",
    "git-issues": "1.2.0",
    "lazy-ass": "0.6.0",
    "mocha": "2.3.4",
    "pkgfiles": "2.3.0",
    "pre-git": "2.0.0",
    "semantic-release": "6.0.3",
    "xplain": "0.6.0"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test",
        "npm run examples"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
